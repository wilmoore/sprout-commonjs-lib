{
  "name": "<%= package_name %>",
  "description": "<%= package_description %>",
  "version": "0.0.0",
  "author": "<%= author_full_name %> <<%= github_user_email %>>",
  "bugs": {
    "url": "https://github.com/<%= github_user_name %>/<%= repo_name %>/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "istanbul": "^0.3.13",
    "nodemon": "^1.3.7",
    "standard": "^3.7.1",
    "tap-spec": "^2.2.2",
    "tape": "^4.0.0",
    "tape-catch": "^1.0.4"
  },
  "homepage": "https://github.com/<%= github_user_name %>/<%= repo_name %>",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "preferGlobal": false,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= github_user_name %>/<%= repo_name %>"
  },
  "scripts": {
    "cover": "istanbul cover test.js",
    "dev": "nodemon -x 'npm run test --silent' -e 'js json'",
    "standard": "standard",
    "test": "npm run standard --silent && node test.js | tap-spec"
  }
}
